<WrapPanel x:Class="QuietNotes.ColorPickRectangle" x:Name="colorPickRectangle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuietNotes"
             mc:Ignorable="d" 
             Width="28" Height="28" Margin="0">
    <Rectangle x:Name="rectColor" Width="26" Height="26" d:Fill="Red" Margin="1" StrokeThickness="0"
               MouseEnter="RectColor_MouseEnter" MouseLeave="RectColor_MouseLeave" MouseLeftButtonDown="RectColor_MouseLeftButtonDown"/>
</WrapPanel>
